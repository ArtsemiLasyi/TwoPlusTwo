<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>TwoPlusTwo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>by.aaproduction.tpt.controller.MainServlet</servlet-name>
  	<servlet-class>by.aaproduction.tpt.controller.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>by.aaproduction.tpt.controller.MainServlet</servlet-name>
  	<url-pattern>/Main</url-pattern>
  </servlet-mapping>
  <error-page>
  	<error-code>404</error-code>
  	<location>/404.html</location>
  </error-page>
  <servlet>
  	<servlet-name>by.aaproduction.tpt.controller.AccountServlet</servlet-name>
  	<servlet-class>by.aaproduction.tpt.controller.AccountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>by.aaproduction.tpt.controller.AccountServlet</servlet-name>
  	<url-pattern>/Registration</url-pattern>
  	<url-pattern>/Login</url-pattern>
  	<url-pattern>/Account</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>by.aaproduction.tpt.controller.NotFoundServlet</servlet-name>
  	<servlet-class>by.aaproduction.tpt.controller.NotFoundServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>by.aaproduction.tpt.controller.NotFoundServlet</servlet-name>
  	<url-pattern>/Notfound</url-pattern>
  </servlet-mapping>
</web-app>